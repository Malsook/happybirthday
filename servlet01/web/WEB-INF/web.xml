<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>servlet01/servlet/student</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>studentServlet</servlet-name>
        <servlet-class>com.powernode.Servlet.StudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentServlet</servlet-name>
        <url-pattern>/sbltz</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>studentServlet2</servlet-name>
        <servlet-class>com.powernode.Servlet.StudentServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentServlet2</servlet-name>
        <url-pattern>/servlet01/servlet/student2</url-pattern>
    </servlet-mapping>
</web-app>